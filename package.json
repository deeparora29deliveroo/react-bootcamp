{
  "name": "@deliveroo/project-name",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "choose:javascript": "yarn install --silent --frozen-lockfile && bash ./scripts/chooseJavascript",
    "choose:typescript": "yarn install --silent --frozen-lockfile && bash ./scripts/chooseTypescript",
    "decision:notMade": "bash ./scripts/decisionNotMade",
    "start": "yarn decision:notMade && cdt start",
    "build": "yarn decision:notMade && yarn build:client && yarn build:server",
    "build:client": "yarn decision:notMade && cdt build",
    "build:server": "yarn decision:notMade && tsc --project tsconfig.server.json && cp app/env.js dist/app/env.js",
    "dev": "yarn decision:notMade && cdt dev",
    "test": "yarn decision:notMade && NODE_ENV=test jest -c ./jest/config.json",
    "lint": "yarn decision:notMade && eslint './app/{client,server}/**/*.{js,jsx,ts,tsx}'",
    "reset-staging": "reset-staging --github-token $GITHUB_TOKEN --slack-channel CHANGE-TO-YOUR-SLACK-CHANNEL"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "dependencies": {
    "@deliveroo/create-deliveroo-tool": "13.0.0",
    "@deliveroo/fetch": "^3.0.1",
    "@deliveroo/logger": "^3.0.0",
    "@deliveroo/tools-component-library": "24.12.1",
    "css-modules-typescript-loader": "^4.0.1",
    "execa": "5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-required-if": "^1.0.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@deliveroo/express-identity": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@deliveroo/eslint-config": "^5.0.0",
    "@deliveroo/prettier-config": "^1.0.0",
    "@deliveroo/reset-staging": "^1.1.0",
    "@deliveroo/tsconfig": "^1.0.0",
    "@types/enzyme": "^3.10.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.4.4",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.0",
    "prettier": "^2.7.1",
    "react-helmet": "^6.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "webpack": "^5.74.0"
  }
}
