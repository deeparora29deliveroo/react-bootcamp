echo "Choosing JavaScript..."
rm -rf typings
rm app/env.d.ts app/client/components/EnvironmentVariablesExamples/styles.scss.d.ts tsconfig.json tsconfig.server.json
node ./scripts/convertImportToRequire.js
babel app -x '.ts,.tsx' -d app --keep-file-extension
bash ./scripts/renameTsToJs
node ./scripts/modifyConfigFiles.js Javascript
node ./scripts/removeDeps.js Javascript
rm .circleci/config.yml
mv .circleci/config-template.yml .circleci/config.yml
sh ./scripts/cleanup.sh